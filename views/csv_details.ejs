<h1>CSV details</h1>

<div>
    <table>
        <thead>
            <% keys.forEach(key =>{ %>
                <th><%= key %></th>
            <% }) %>
        </thead>
        <tbody>
            <% data.forEach(record =>{ %>
                <tr>
                    <% for (let i=0;i < keys.length;i++){%>
                    <td><%= record[keys[i]]%></td>
                    <% }%>
                </tr>
                
            <% }) %>
        </tbody>
    </table>

</div>